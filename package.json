    "@tarojs/taro": "4.0.0-canary.6",
    "@tarojs/taro-rn": "4.0.0-canary.6",
    "@tarojs/plugin-framework-react": "4.0.0-canary.6",
    "@tarojs/react": "4.0.0-canary.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.73.1"
