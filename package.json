    "@tarojs/components": "4.0.0-canary.6",
    "@tarojs/helper": "4.0.0-canary.6",
    "@tarojs/plugin-platform-weapp": "4.0.0-canary.6",
    "@tarojs/plugin-platform-alipay": "4.0.0-canary.6",
    "@tarojs/plugin-platform-tt": "4.0.0-canary.6",
    "@tarojs/plugin-platform-swan": "4.0.0-canary.6",
    "@tarojs/plugin-platform-jd": "4.0.0-canary.6",
    "@tarojs/plugin-platform-qq": "4.0.0-canary.6",
    "@tarojs/plugin-platform-h5": "4.0.0-canary.6",
    "@tarojs/rn-supporter": "4.0.0-canary.6",
    "@tarojs/runtime": "4.0.0-canary.6",
