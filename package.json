    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "4.0.0-canary.6",
    "eslint": "^8.19.0",
    "eslint-config-taro": "4.0.0-canary.6",
    "eslint-plugin-import": "^2.12.0",
