    "ios": "react-native run-ios",
    "start": "react-native start --custom-log-reporter-path @tarojs/rn-supporter/TerminalReporter.js",
    "podInstall": "pod-install",
    "upgradePeerdeps": "install-peerdeps @tarojs/taro-rn@4.0.0-canary.2 -o -Y && install-peerdeps @tarojs/components-rn@4.0.0-canary.2 -o -Y && install-peerdeps @tarojs/router-rn@4.0.0-canary.2 -o -Y && pod-install"
